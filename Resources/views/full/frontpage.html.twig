{% extends noLayout ? viewbaseLayout : "tfkfarteBundle::pagelayout.html.twig" %}

{% block content %}
<div class="content background-front">

<div class="constrained-content">
{% include 'tfkfarteBundle:frontpage:reiseplanlegger.html.twig' %}
</div>

</div>
<div class="constrained-content">
  <div class="campaign-content">
  {{ render(
    controller(
      "ez_content:viewContent",
        {
          "contentId": 195,
          "viewType": "line"
        }
    )
  ) }}
  </div>
<div class="grid grid--12">
  <div class="grid__item grid__item--4-12 media item item--horizontal">
   {{ render(
    controller(
      "ez_content:viewContent",
        {
          "contentId": 186,
          "viewType": "line",
          "params": {
            "tabboks_size":"large"
          }
        }
    )
  ) }}
    {{ render( controller("tfkfarteBundle:Item:messages", {'locationId': 189,  'params' : { 'class' : ['article'] , 'viewType': 'list', 'limit': '3' }} )) }}
  </div>
    <div class="grid__item grid__item--4-12 media item item--horizontal">
       {{ render(
    controller(
      "ez_content:viewContent",
        {
          "contentId": 187,
