.item {
    position: initial;
    padding: 1em 0;
    overflow: hidden;
}
.item__content {
    overflow: hidden;
    padding-left: 0.5em;
}
.campaign-content {
    background: #FFD520;
    padding-right: 2em;
}
.campaign-content p {
    font-size: 2.9em;
    line-height: 1.9;
    padding: .2em 0 0 0.5em;
    font-family: "Telefon", "Trebuchet MS", "Lucida Grande", "Lucida Sans Unicode", "Lucida Sans", Tahoma, sans-serif;
}
.page-header__logo {
    margin: 1.5em 0;
}

.menu {
    background: #000;
}

.menu__item-link {
    display: block;
    color: #fff;
    -moz-transition: background .2s;
    -o-transition: background .2s;
    -webkit-transition: background .2s;
    transition: background .2s;
    text-decoration: none;
}

.menu__item-link:hover, .menu__item-link:focus, .menu__item-link:active
.page-header {
    -webkit-transition: all 100ms ease;
    -moz-transition: all 100ms ease;
    -ms-transition: all 100ms ease;
    -o-transition: all 100ms ease;
    transition: all 100ms ease;
    text-decoration: none;
    background: #6ac4ae;
    color: #333;
}

.menu__submenu {
    background-color: #000;
    }
.page-header__logo img {
    display: block;
}


.pagination {
    position: relative;
    text-align: center;
}

.pagination ul {
    list-style: none;
    padding-left: 85px;
    padding-right: 85px;
}

.pagination li {
    display: inline-block;
    margin: 5px;
}

.pagination .prev {
    position: absolute;
    left: 0;
}

.pagination .prev a:before {
    color: #383838;
    font-size: 27px;
    margin-left: -13px;
    position: absolute;
}

.pagination .next {
    position: absolute;
    right: 0;
}

.pagination .next a:after {
    color: #383838;
    font-size: 27px;
    margin-left: 4px;
    position: absolute;
}

.pagination .next span, .pagination .prev span {
    background-color: #e8f3f0;
    border: 0 none;
    color: #6c6c6c;
    display: inline-block;
    padding: 0.5em 1em;
}

.pagination a {
    background-color: #e8f3f0;
    border: 0 none;
    color: #000;
    cursor: pointer;
    display: inline-block;
    padding: 0.5em 1em;
    text-decoration: underline;
}

.pagination li.active span {
    background-color: #ffd520;
    color: #000;
    border: 0 none;
    display: inline-block;
    padding: 0.5em 1em;
}

.pagination__next a, .pagination__prev a {
    padding: 0;
}

.main .pagination {
    float: left;
    width: 100%;
}

