#ui-datepicker-div {
    background-color: #fff;
    border:1px solid #bdbdbd;
    border-radius: 3px 3px 3px 3px;
    box-shadow: 0 2px 3px 0 rgba(51, 51, 51, .2);
    color: #333;
    margin-left: -85px;
    max-width: 280px;
    width: 100%;
    display: none;
     font-family: "Telefon", "Trebuchet MS", "Lucida Grande", "Lucida Sans Unicode", "Lucida Sans", Tahoma, sans-serif;
}
#ui-datepicker-div .ui-datepicker-header {
    background-color: #fff;
    border-bottom: 1px solid #fff;
    box-shadow: 0 1px 0 0 #fff;
    display: inline-block;
    width: 100%
}
#ui-datepicker-div .ui-datepicker-header a.ui-datepicker-prev {
    color: #333;
    float: left;
    padding: 2%;
    text-align: center;
    width: 20%
}
#ui-datepicker-div .ui-datepicker-header a.ui-datepicker-next {
    color: #333;
    float: right;
    padding: 2%;
    text-align: center;
    width: 20%
}
#ui-datepicker-div .ui-datepicker-header .ui-datepicker-title {
    border-left: 1px solid #333;
    border-right: 1px solid #333;
    box-shadow: -1px 0 0 0 #333 inset, 1px 0 0 0 #333 inset;
    float: left;
    font-weight: 500;
    padding: 2%;
    text-align: center;
    width: 60%;
    background-color: #ededed;
}
#ui-datepicker-div a:hover {
    background-color: #ffd520;
    border: medium none;
    cursor: pointer
}
#ui-datepicker-div .ui-datepicker-calendar thead {
    border-bottom: 1px solid #333;
    box-shadow: 0 1px 0 0 #333;
    height: 30px
}
#ui-datepicker-div .ui-datepicker-calendar thead tr {
    float: left;
    width: 100%
}
#ui-datepicker-div .ui-datepicker-calendar thead tr th {
    width: 40px
}
#ui-datepicker-div .ui-datepicker-calendar thead tr th:last-child {
    color: red
}
#ui-datepicker-div .ui-datepicker-calendar tbody {
    background-color: #fff
}
#ui-datepicker-div .ui-datepicker-calendar tbody tr {
    float: left;
    width: 100%
}
#ui-datepicker-div .ui-datepicker-calendar tbody tr td {
    border-bottom: 1px solid #999;
    border-right: none;
    box-shadow: none;
    display: table-cell;
    float: none;
    height: 30px;
    vertical-align: middle;
    width: 40px
}
#ui-datepicker-div .ui-datepicker-calendar tbody tr td a {
    color: #333;
    display: block;
    height: 100%;
    padding: 9%;
    text-align: center;
    width: 100%
}
#ui-datepicker-div .ui-datepicker-calendar tbody tr td:last-child {
    border-right: medium none
}
#farte-reiseplanlegger {
    margin: 1em 0;
    background-color: #fff;
    padding: 0em 2em 0em 2em;
    display: block;
    border-radius: 5px;
}
#farte-reiseplanlegger label {
    font-size: 1.5em;
    line-height: 2em;
    display: block;
         font-family: "Telefon", "Trebuchet MS", "Lucida Grande", "Lucida Sans Unicode", "Lucida Sans", Tahoma, sans-serif;
}

div#farte-reiseplanlegger input {
    height: 2.5em;
    background-color: #f3f4f5;
    border: none;
    width: 100%
}

@media screen and (min-width: 856px) {
    #farte-reiseplanlegger {
        margin: 0;
        margin-bottom: 1em;
        display: block
    }
}

#farte-reiseplanlegger .autocomplete {
    display: block;
    position: fixed;
    width: 50%;
    background-color: #fff;
}
#farte-reiseplanlegger .autocomplete .ui-autocomplete {

}
#farte-reiseplanlegger .autocomplete .ui-autocomplete li {
    padding: .3em .6em;
    list-style: none;
}


.ui-autocomplete {
    top: 0;
    left: 0;
    cursor: default;
    background-color: #fff;
    box-shadow: none;
    max-height: 500px;
    overflow: none;
    text-align: left;
    z-index: 99999;
    width: 90%
}
.ui-autocomplete li:focus,
.ui-autocomplete li:hover {
    background-color: #6AC4AE;
    color: #f9f9f9
}
.ui-autocomplete a {
    color: #FFF;
    text-shadow: 0 -1px 0 #68a03d;
    text-decoration: none;
    display: block;
    padding: 5px 28px 5px 5px;
    position: relative
}
.ui-helper-hidden-accessible {
    display: none
}
.ui-state-focus {
    background-color: #6AC4AE
}

.ui-timepicker-wrapper {
    overflow-y: auto;
    height: 150px;
    width: 6.5em;
    background: #fff;
    border: 1px solid #ddd;
    box-shadow: 0 5px 10px rgba(0, 0, 0, .2);
    outline: 0;
    z-index: 10001;
    margin: 0
}
.ui-timepicker-wrapper.ui-timepicker-with-duration {
    width: 11em
}
.ui-timepicker-list {
    margin: 0;
    padding: 0;
    list-style: none
}
.ui-timepicker-duration {
    margin-left: 5px;
    color: #888
}
.ui-timepicker-list:hover .ui-timepicker-duration {
    color: #888
}
.ui-timepicker-list li {
    padding: 3px 0 3px 5px;
    cursor: pointer;
    white-space: nowrap;
    color: #000;
    list-style: none;
    margin: 0
}
.ui-timepicker-list:hover .ui-timepicker-selected {
    background: #fff;
    color: #000
}
.ui-timepicker-list .ui-timepicker-selected:hover,
.ui-timepicker-list li:hover,
li.ui-timepicker-selected {
    background: #7BC142;
    color: #fff
}
.ui-timepicker-list li:hover .ui-timepicker-duration,
li.ui-timepicker-selected .ui-timepicker-duration {
    color: #ccc
}
.ui-timepicker-list li.ui-timepicker-disabled,
.ui-timepicker-list li.ui-timepicker-disabled:hover,
.ui-timepicker-list li.ui-timepicker-selected.ui-timepicker-disabled {
    color: #888;
    cursor: default
}
.ui-timepicker-list li.ui-timepicker-disabled:hover,
.ui-timepicker-list li.ui-timepicker-selected.ui-timepicker-disabled {
    background: #f2f2f2
}
.ht_knapp .gronn {
    background-color: #7BC142
}
.ht_knapp .gronn a,
.ht_knapp .gronn h1 {
